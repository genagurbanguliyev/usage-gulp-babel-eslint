{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2015": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": [
            1,
            "always-multiline"
        ],
        "indent": [
            2,
            4
        ]
    }
}
